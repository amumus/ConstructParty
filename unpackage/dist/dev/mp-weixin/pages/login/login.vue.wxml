<template name="02ce32d2">
  <view class="_view data-v-80eff750 content">
    <view class="_view data-v-80eff750 input-group">
      <view class="_view data-v-80eff750 input-row border">
        <text class="_text data-v-80eff750 title">账号：</text>
        <input type="text" value="{{account}}" bindinput="handleProxy" placeholder="请输入账号" data-eventid="{{'LXL-0'}}" data-comkey="{{$k}}" class="_input data-v-80eff750" />
      </view>
      <view class="_view data-v-80eff750 input-row">
        <text class="_text data-v-80eff750 title">密码：</text>
        <input type="text" password="true" value="{{password}}" bindinput="handleProxy" placeholder="请输入密码" data-eventid="{{'n8Z-1'}}" data-comkey="{{$k}}" class="_input data-v-80eff750" />
      </view>
    </view>
    <view class="_view data-v-80eff750 btn-row">
      <button type="primary" class="_button data-v-80eff750 primary" bindtap="handleProxy" data-eventid="{{'ZDE-2'}}" data-comkey="{{$k}}">登录</button>
    </view>
    <view class="_view data-v-80eff750 action-row">
      <navigator url="../reg/reg" class="_navigator data-v-80eff750">注册账号</navigator>
      <text class="_text data-v-80eff750">|</text>
      <navigator url="../pwd/pwd" class="_navigator data-v-80eff750">忘记密码</navigator>
    </view>
    <view class="_view data-v-80eff750 oauth-row" wx:if="{{hasProvider}}" style=" {{('top:' + (positionTop + 'px') + ';')}}">
      <view class="_view data-v-80eff750 oauth-image" wx:key="provider.value" key="{{provider.value}}" wx:for="{{providerList}}" wx:for-index="index" wx:for-item="provider">
        <image src="{{provider.image}}" bindtap="handleProxy" data-eventid="{{'1Rg-3-'+index}}" data-comkey="{{$k}}" class="_image data-v-80eff750"></image>
      </view>
    </view>
  </view>
</template>