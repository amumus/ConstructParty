<import src="/components/slots" />
<template name="ace860b6">
  <view class="_view data-v-2f3c65a4 uni-card" bindtap="handleProxy" data-eventid="{{'Muq-0'}}" data-comkey="{{$k}}">
    <template name="default"></template>
    <view class="_view data-v-2f3c65a4 uni-card__header" wx:if="{{title}}">
      <view class="_view data-v-2f3c65a4 uni-card__header-extra-img-view" wx:if="{{thumbnail}}">
        <image class="_image data-v-2f3c65a4 uni-card__header-extra-img" src="{{thumbnail}}"></image>
      </view>
      <view class="_view data-v-2f3c65a4 uni-card__header-title-text">{{title}}</view>
      <view class="_view data-v-2f3c65a4 uni-card__header-extra-text" wx:if="{{extra}}">{{extra}}</view>
    </view>
    <view class="_view data-v-2f3c65a4 uni-card__content {{isFull === false || isFull === 'false' ? 'uni-card__content--pd' : ''}}">
      <template data="{{...$root['0'], ...$root[$k], $root}}" is="{{$slotdefault || 'default'}}"></template>
    </view>
    <view class="_view data-v-2f3c65a4 uni-card__footer" wx:if="{{note}}">{{note}}</view>
  </view>
</template>